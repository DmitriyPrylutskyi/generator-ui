{
    "name": "@lectrum/generator-ui",
    "version": "0.0.2",
    "description": "A generator for creating cutting-edge frontend single page applications environments.",
    "repository": "https://github.com/Lectrum/generator-lectrum-ui",
    "author": {
        "name": "Dima Vakatsiienko",
        "email": "dvakatsiienko@lectrum.io"
    },
    "scripts": {
        "start": "nodemon",
        "build": "npx babel ./source/index.js --out-file ./generators/app/index.js",
        "flowtype": "yarn flow",
        "flow-coverage": "run-s flow-coverage:generate flow-coverage:clean",
        "flow-coverage:generate": "flow-coverage-report --config \"./flow-coverage.config.json\"",
        "lint": "run-s lint:javascript",
        "lint:javascript": "jest --projects \"./jest/jest.eslint.config.json\"",
        "soundcheck": "run-s flowtype lint",
        "commitmsg": "commitlint -E GIT_PARAMS"
    },
    "dependencies": {
        "chalk": "^2.1.0",
        "yeoman-generator": "^3.1.1",
        "yosay": "^2.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/node": "^7.0.0",
        "@babel/plugin-transform-modules-commonjs": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@lectrum/config-commitlint": "^1.0.8",
        "@lectrum/eslint-config-core": "^1.0.18",
        "babel-eslint": "^9.0.0",
        "babel-plugin-add-module-exports": "^0.2.1",
        "cz-conventional-changelog": "^2.1.0",
        "eslint": "^5.4.0",
        "eslint-plugin-babel": "^5.1.0",
        "eslint-plugin-flowtype": "^2.50.0",
        "eslint-plugin-jest": "^21.22.0",
        "eslint-plugin-prettier": "^2.6.0",
        "eslint-plugin-react": "^7.11.1",
        "eslint-plugin-react-redux": "^2.3.0",
        "flow-bin": "^0.79.1",
        "flow-coverage-report": "^0.5.0",
        "husky": "^0.14.3",
        "jest": "^23.5.0",
        "jest-runner-eslint": "^0.6.0",
        "nodemon": "^1.18.4",
        "npm-run-all": "^4.1.3",
        "yeoman-assert": "^3.1.0",
        "yeoman-test": "^1.9.1"
    },
    "engines": {
        "npm": ">= 4.0.0"
    },
    "files": [
        "generators"
    ],
    "main": "generators/index.js",
    "keywords": [
        "yeoman-generator",
        "lectrum"
    ],
    "license": "MIT"
}
